{
  "name": "ramen",
  "version": "0.0.1",
  "description": "MongoDB, ExpressJS, REACT, & NodeJS Starter Project",
  "author": "Terry J. Owen <terry@iknowmac.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run build:clean & nodemon ./src/server/bin/www local & npm run webpack:dev:server",
    "prebuild": "npm run build:clean && npm run build:html",
    "build": "export NODE_ENV=production & NODE_ENV=production & webpack --config ./src/config/webpack.config.prod.js --progress",
    "build:clean": "node ./bin/clean-build.js createdir",
    "build:html": "node ./bin/html-build.js",
    "webpack:dev:server": "webpack-dev-server --config ./src/config/webpack.config.dev.js --hot --inline --progress",
    "lint": "eslint config src/** --ext .js",
    "test": "mocha test --compilers js:babel-core/register --recursive",
    "seed": "node ./src/server/db/seeds"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/towen/ramen.git"
  },
  "keywords": [
    "mongo",
    "express",
    "react",
    "node",
    "boilerplate",
    "javascript"
  ],
  "dependencies": {
    "body-parser": "^1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "^2.6.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.17.1",
    "mongoose": "^4.8.1",
    "morgan": "^1.8.0",
    "node-sass-middleware": "^0.11.0",
    "nodemon": "^1.11.0",
    "nunjucks": "^3.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-immutable-state-invariant": "^1.2.4",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "^2.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-react-require": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "chai-fs": "^1.0.0",
    "chai-http": "^3.0.0",
    "cheerio": "^0.22.0",
    "colors": "^1.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "imports-loader": "^0.7.0",
    "jest": "^18.1.0",
    "jsdom": "^9.10.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
