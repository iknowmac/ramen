{
  "name": "ramen",
  "version": "0.0.1",
  "description": "MongoDB, ExpressJS, REACT, & NodeJS Starter Project",
  "author": "Terry J. Owen <terry@iknowmac.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "node ./src/server/bin/www local & npm run webpack:dev",
    "webpack:dev": "webpack-dashboard -- webpack-dev-server --config ./src/config/webpack.config.dev.js --hot --inline --progress --colors",
    "build": "npm run build:clean && npm run build:server && npm run build:webpack:prod",
    "build:clean": "rm -rf build && mkdir -p build && mkdir -p build/server && mkdir -p build/assets",
    "build:server": "babel src/server --out-dir build/server --copy-files",
    "build:prod": "npm run build:clean && npm run build:server && npm run build:webpack:prod",
    "build:webpack:prod": "webpack --progress --config src/config/webpack.config.prod.js",
    "test": "mocha test --compilers js:babel-core/register --recursive",
    "seed": "node ./src/server/db/seeds"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/towen/ramen.git"
  },
  "keywords": [
    "mongo",
    "express",
    "react",
    "node",
    "boilerplate",
    "javascript"
  ],
  "dependencies": {
    "body-parser": "^1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "^2.6.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "material-design-lite": "^1.3.0",
    "moment": "^2.17.1",
    "mongoose": "^4.8.1",
    "morgan": "^1.8.0",
    "node-sass-middleware": "^0.11.0",
    "nunjucks": "^3.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-immutable-state-invariant": "^1.2.4",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "^2.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-require": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "jsdom": "^9.10.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "mocha-jsdom": "^1.1.0",
    "mongoose-seed-plus": "^0.1.2",
    "postcss-loader": "^1.2.2",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "redux-logger": "^2.8.1",
    "redux-slider-monitor": "^1.0.7",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.3.0",
    "webpack-dotenv-plugin": "^2.0.0"
  }
}
